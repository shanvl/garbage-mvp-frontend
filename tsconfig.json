{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitReturns": true,
    "noEmitOnError": true
  }
}
